<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="ru.vn.georepect.cbt" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>flashmob</name>
    <description>flashmob</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">dimkk</author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <access origin="*://*.facebook.net/*" subdomains="true" />
    <access origin="*://*.facebook.com/*" subdomains="true" />
    <access launch-external="yes" origin="geo:*" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="loadUrlTimeoutValue" value="600000" />
    <allow-intent href="geo:*" />
    <vs:features />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
