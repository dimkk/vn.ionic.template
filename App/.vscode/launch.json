{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceRoot}/www/index.html"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8100",
			"sourceMaps": true,
			"webRoot": "www"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 8100
		}
	]
}